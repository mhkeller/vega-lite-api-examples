{
  "compilerOptions": {
    "checkJs": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strict": true,
    "module": "NodeNext",
    "target": "ESNext",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true
  },
  "skipLibCheck": true
}
